{
  "name": "@toruslabs/solana-embed",
  "version": "3.0.0",
  "description": "Embed script for solana blockchain",
  "main": "dist/lib.cjs/index.js",
  "module": "dist/lib.esm/index.js",
  "unpkg": "dist/solanaEmbed.umd.min.js",
  "jsdelivr": "dist/solanaEmbed.umd.min.js",
  "types": "dist/lib.cjs/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "torus-scripts start",
    "build": "torus-scripts build",
    "build:test": "cross-env NODE_ENV=testing torus-scripts build",
    "release": "torus-scripts release",
    "prepack": "npm run build",
    "lint:ts": "eslint --fix 'src/**/*.ts'",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/torusresearch/solana-embed.git"
  },
  "dependencies": {
    "@web3auth/ws-embed": "^5.0.17"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.x",
    "@babel/runtime": "7.x"
  },
  "devDependencies": {
    "@solana/web3.js": "^1.98.2",
    "@babel/runtime": "^7.28.2",
    "@rollup/plugin-replace": "^6.0.2",
    "@toruslabs/config": "^3.1.0",
    "@toruslabs/eslint-config-typescript": "^4.1.4",
    "@toruslabs/torus-scripts": "^7.1.6",
    "@types/node": "^22",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "webpack": "^5.101.0"
  },
  "mocha": {
    "timeout": 0
  },
  "keywords": [
    "torus",
    "wallet",
    "Solana",
    "embed",
    "login",
    "OAuth",
    "crypto"
  ],
  "author": "Torus Labs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/torusresearch/solana-embed/issues"
  },
  "homepage": "https://github.com/torusresearch/solana-embed#readme",
  "lint-staged": {
    "!(*d).{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json, md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.x",
    "npm": ">=9.x"
  }
}
