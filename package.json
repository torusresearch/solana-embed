{
  "name": "@toruslabs/solana-embed",
  "version": "0.0.7",
  "description": "Embed script for solana blockchain",
  "main": "dist/torus.cjs.js",
  "module": "dist/torus.esm.js",
  "unpkg": "dist/torus.umd.min.js",
  "jsdelivr": "dist/torus.umd.min.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "rimraf dist/ && cross-env NODE_ENV=development rollup -c && parallel-webpack --no-stats",
    "build": "rimraf dist/ && cross-env NODE_ENV=production rollup -c && parallel-webpack --no-stats",
    "build:test": "rimraf dist/ && cross-env NODE_ENV=testing rollup -c && parallel-webpack --no-stats",
    "prepack": "npm run build",
    "lint:ts": "eslint --fix 'src/**/*.ts'",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/torusresearch/solana-embed.git"
  },
  "peerDependencies": {
    "@babel/runtime": "7.x"
  },
  "dependencies": {
    "@solana/web3.js": "^1.29.2",
    "@toruslabs/base-controllers": "^1.2.2",
    "@toruslabs/http-helpers": "^1.4.0",
    "@toruslabs/openlogin-jrpc": "^1.3.1",
    "@types/bs58": "^4.0.1",
    "eth-rpc-errors": "^4.0.3",
    "fast-deep-equal": "^3.1.3",
    "is-stream": "^3.0.0",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.7.1",
    "pump": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@commitlint/is-ignored": "^15.0.0",
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@babel/runtime": "^7.15.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/create-hash": "^1.2.2",
    "@types/node": "^14.7.0",
    "@types/once": "^1.4.0",
    "@types/pump": "^1.1.1",
    "@types/readable-stream": "^2.3.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "empty-module": "0.0.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-problems": "^5.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-webpack-plugin": "^2.5.4",
    "express": "^4.17.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "mocha": "^9.1.2",
    "parallel-webpack": "^2.6.0",
    "prettier": "^2.4.1",
    "puppeteer": "^10.4.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.0"
  },
  "mocha": {
    "timeout": 0
  },
  "keywords": [
    "torus",
    "wallet",
    "Solana",
    "embed",
    "login",
    "OAuth",
    "crypto"
  ],
  "author": "Torus Labs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/torusresearch/solana-embed/issues"
  },
  "homepage": "https://github.com/torusresearch/solana-embed#readme",
  "lint-staged": {
    "!(*d).{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json, md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.17.0",
    "npm": ">=6.x"
  }
}
